                                  owl:onProperty :defaultBranch ;
                                  owl:onClass :Branch ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :codebase ;
                                  owl:onClass rdfs:Resource ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :defaultBranchName ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
